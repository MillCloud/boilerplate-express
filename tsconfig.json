{
  "extends": "./node_modules/@modyqyw/fabric/tsconfig.base.json",
  "compilerOptions": {
    "noUnusedParameters": false,
    "baseUrl": ".",
    "lib": ["ESNext"],
    "target": "ESNext",
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["@types/express-serve-static-core", "@types/jest", "@types/qs", "type-fest"]
  },
  "include": [
    "./**/.*.js",
    "./**/.*.cjs",
    "./**/.*.mjs",
    "./**/*.js",
    "./**/*.cjs",
    "./**/*.mjs",
    "./**/*.jsx",
    "./**/.*.ts",
    "./**/.*.cts",
    "./**/.*.mts",
    "./**/*.ts",
    "./**/*.cts",
    "./**/*.mts",
    "./**/*.tsx",
    "./**/*.vue",
    "./**/*.svelte"
  ],
  "exclude": [".cache", ".temp", ".tmp", "dist*", "node_modules"]
}
