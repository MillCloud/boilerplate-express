{
  "name": "boilerplate-express",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for express.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "dev": "nodemon ./src/index.ts --watch ./src",
    "dev:https": "cross-env HTTPS=true nodemon ./src/index.ts --watch ./src",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "prod": "cross-env NODE_ENV=production nodemon ./src/index.ts --watch ./src",
    "prod:https": "cross-env NODE_ENV=production HTTPS=true nodemon ./src/index.ts --watch ./src"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "esmo --experimental-specifier-resolution=node"
    }
  },
  "dependencies": {
    "@modyqyw/utils": "~2.1.2",
    "body-parser": "~1.19.1",
    "cls-rtracer": "~2.6.0",
    "compression": "~1.7.4",
    "content-disposition": "~0.5.4",
    "cors": "~2.8.5",
    "dayjs": "~1.10.7",
    "dotenv": "~15.0.0",
    "dotenv-expand": "~6.0.1",
    "express": "~4.17.2",
    "express-rate-limit": "~6.2.0",
    "express-static-gzip": "~2.1.1",
    "express-validator": "~6.14.0",
    "express-winston": "~4.2.0",
    "formidable": "~2.0.1",
    "got": "~12.0.1",
    "helmet": "~5.0.2",
    "jsonwebtoken": "~8.5.1",
    "mime-types": "~2.1.34",
    "mongoose": "~6.1.9",
    "node-cron": "~3.0.0",
    "picocolors": "~1.0.0",
    "query-string": "~7.1.0",
    "shelljs": "~0.8.5",
    "spdy": "~4.0.2",
    "statuses": "~2.0.1",
    "winston": "~3.5.1",
    "winston-daily-rotate-file": "~4.6.0"
  },
  "devDependencies": {
    "@babel/core": "~7.16.12",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~16.1.0",
    "@modyqyw/fabric": "~4.10.4",
    "@types/body-parser": "~1.19.2",
    "@types/compression": "~1.7.2",
    "@types/content-disposition": "~0.5.4",
    "@types/cors": "~2.8.12",
    "@types/express": "~4.17.13",
    "@types/formidable": "~2.0.4",
    "@types/jsonwebtoken": "~8.5.8",
    "@types/node-cron": "~3.0.1",
    "@types/serve-static": "~1.13.10",
    "@types/shelljs": "~0.8.11",
    "@types/spdy": "~3.4.5",
    "@types/statuses": "~2.0.0",
    "@types/supertest": "~2.0.11",
    "@typescript-eslint/eslint-plugin": "~5.10.2",
    "@typescript-eslint/parser": "~5.10.2",
    "commitizen": "~4.2.4",
    "cross-env": "~7.0.3",
    "eslint": "~8.8.0",
    "esno": "~0.14.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.3.2",
    "markdownlint-cli": "~0.30.0",
    "nodemon": "~2.0.15",
    "pm2": "~5.1.2",
    "prettier": "~2.5.1",
    "release-it": "^14.12.4",
    "supertest": "~6.2.2",
    "type-fest": "~2.11.1",
    "typescript": "~4.5.5"
  }
}
