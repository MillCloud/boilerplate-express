{
  "name": "boilerplate-express",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for express.",
  "type": "module",
  "scripts": {
    "commit": "git-cz",
    "dev": "nodemon ./src/index.ts --watch ./src",
    "dev:https": "cross-env HTTPS=true nodemon ./src/index.ts --watch ./src",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "prod": "cross-env NODE_ENV=production nodemon ./src/index.ts --watch ./src",
    "prod:https": "cross-env NODE_ENV=production HTTPS=true nodemon ./src/index.ts --watch ./src"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "esmo --experimental-specifier-resolution=node"
    }
  },
  "dependencies": {
    "@awaitjs/express": "~0.9.0",
    "@modyqyw/utils": "~2.1.2",
    "body-parser": "~1.19.1",
    "cls-rtracer": "~2.6.0",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "csurf": "~1.11.0",
    "dayjs": "~1.10.7",
    "express": "~4.17.2",
    "express-http-context": "~1.2.4",
    "express-partial-response": "~1.0.4",
    "express-validator": "~6.14.0",
    "express-winston": "~4.2.0",
    "formidable": "~2.0.1",
    "helmet": "~5.0.1",
    "mime-types": "~2.1.34",
    "mongoose": "~6.1.6",
    "nanoid": "~3.2.0",
    "node-cron": "~3.0.0",
    "node-fetch": "~3.1.1",
    "picocolors": "~1.0.0",
    "query-string": "~7.1.0",
    "response-time": "~2.3.2",
    "serve-static": "~1.14.2",
    "shelljs": "~0.8.5",
    "spdy": "~4.0.2",
    "static-expiry": "~0.0.11",
    "timeout": "~0.2.1",
    "winston": "~3.4.0",
    "winston-daily-rotate-file": "~4.5.5"
  },
  "devDependencies": {
    "@babel/core": "~7.16.7",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~16.0.2",
    "@modyqyw/fabric": "~4.8.0",
    "@types/body-parser": "~1.19.2",
    "@types/compression": "~1.7.2",
    "@types/cors": "~2.8.12",
    "@types/csurf": "~1.11.2",
    "@types/express": "~4.17.13",
    "@types/formidable": "~2.0.3",
    "@types/node-cron": "~3.0.1",
    "@types/response-time": "~2.3.5",
    "@types/serve-static": "~1.13.10",
    "@types/shelljs": "~0.8.11",
    "@types/spdy": "~3.4.5",
    "@types/supertest": "~2.0.11",
    "@types/timeout": "~0.2.0",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "commitizen": "~4.2.4",
    "cross-env": "~7.0.3",
    "eslint": "~8.7.0",
    "esno": "~0.14.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.7",
    "markdownlint-cli": "~0.30.0",
    "nodemon": "~2.0.15",
    "prettier": "~2.5.1",
    "supertest": "~6.2.1",
    "tsconfig-paths": "~3.12.0",
    "type-fest": "~2.9.0",
    "typescript": "~4.5.4"
  }
}
